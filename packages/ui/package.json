{
  "name": "@kyber/ui",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/index.css",
    "./portal": "./src/portal.tsx",
    "./skeleton": "./src/components/ui/skeleton.tsx",
    "./dialog": "./src/components/ui/dialog.tsx",
    "./slider": "./src/components/ui/slider.tsx",
    "./accordion": "./src/components/ui/accordion.tsx",
    "./tooltip": "./src/components/Tooltip/index.tsx",
    "./toggle": "./src/components/toggle.tsx",
    "./scroll-area": "./src/components/ui/scroll-area.tsx",
    "./info-helper": "./src/components/info-helper.tsx",
    "./button": "./src/components/ui/button.tsx",
    "./checkbox": "./src/components/ui/checkbox.tsx",
    "./input": "./src/components/ui/input.tsx",
    "./label": "./src/components/ui/label.tsx",
    "./tabs": "./src/components/ui/tabs.tsx",
    "./card": "./src/components/ui/card.tsx",
    "./radio-group": "./src/components/ui/radio-group.tsx",
    "./token-logo": "./src/components/token-logo.tsx",
    "./locales/*": "./src/locales/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "pnpm run i18n:compile && tsup && node build-css.mjs",
    "type-check": "tsc --noEmit",
    "i18n": "pnpm run i18n:extract && pnpm run i18n:compile",
