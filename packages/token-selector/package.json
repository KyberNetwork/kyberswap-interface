    "@kyber/eslint-config": "workspace:*",
    "@kyber/svgr-esbuild-plugin": "workspace:^",
    "@kyber/tailwind-config": "workspace:^",
    "@kyber/typescript-config": "workspace:*",
    "@lingui/cli": "^4.6.0",
    "@lingui/macro": "^4.6.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.11.24",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.0.17",
